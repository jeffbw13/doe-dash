.slice(2, 5);\n\n  return (\n    <>\n      <div className=\"show-5-stores\">\n        <div className=\"show-5-stores-left\">\n          <h2>{showing.title}</h2>\n        </div>\n        <div className=\"show-5-stores-right\">\n          <div className=\"show-5-stores-top\">\n            {stores2.map((store) => {\n              return (\n                <StoreCard\n                  store={store}\n                  key={store._id}\n                  handleStoreClick={handleStoreClick}\n                />\n              );\n            })}\n          </div>\n          <div className=\"show-5-stores-bottom\">\n            {stores3.map((store) => {\n              return (\n                <StoreCard\n                  store={store}\n                  key={store._id}\n                  handleStoreClick={handleStoreClick}\n                />\n              );\n            })}\n          </div>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Show5Stores;\n","/Users/jeff/Documents/projects/doe-dash/src/library/getStores.js",[],"/Users/jeff/Documents/projects/doe-dash/src/components/StoreCard.js",["173"],"const StoreCard = ({ store, handleStoreClick }) => {\n  const blackOwned = store.blackOwned ? \"Black owned\" : \"\";\n  return (\n    <>\n      <div className=\"store-card\" onClick={() => handleStoreClick(store)}>\n        <div className=\"store-card-img\">\n          <img src={store.photo} />\n        </div>\n        <div className=\"store-card-text\">\n          <p className=\"card-name\">{store.name}</p>\n          <p className=\"card-text\">{store.description}</p>\n          {`${store.stars} stars     ${blackOwned}`}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default StoreCard;\n","/Users/jeff/Documents/projects/doe-dash/src/components/Store.js",["174","175","176"],"import React, { useState } from \"react\";\nimport Products from \"./Products\";\nimport Cart from \"./Cart\";\nimport star from \"../assets/icons/star.svg\";\n\n//  Products here is the menu\n\nconst Store = ({ store, order, setOrder }) => {\n  const orderItem = (item) => {\n    //  if different store, delete the old order\n    if (order.store && order.store.storeId !== store.storeId) {\n      const newOrder = { cartContents: [] };\n      setOrder(newOrder);\n    }\n    const orderX = { ...order };\n    if (!orderX.store) {\n      orderX.store = {\n        _id: store._id,\n        storeId: store.storeId,\n        name: store.name,\n      };\n    }\n    orderX.cartContents.push({ ...item });\n    setOrder(orderX);\n  };\n\n  let dollars = \"\";\n  for (let x = 1; x <= store.cost; x++) {\n    dollars += \"$\";\n  }\n\n  return (\n    <div className=\"store\">\n      <h4>Store</h4>\n      <img src={store.photo} />\n      <div className=\"store-info\">\n        <h1>{store.name}</h1>\n        {store.description.substring(0, 40)}... • {store.stars}\n        <img\n          style={{ width: \"15px\", height: \"15px\", fill: \"#191919\" }}\n          src={star}\n        />\n        ({store.ratings} ratings) • {store.distance} mi • {dollars}\n        <h2>Menu</h2>\n      </div>\n      <Products store={store} orderItem={orderItem} />\n      <div className=\"store-cart\">\n        <Cart order={order} setOrder={setOrder} />\n      </div>\n    </div>\n  );\n};\n\nexport default Store;\n\n// <Products store={store} />\n","/Users/jeff/Documents/projects/doe-dash/src/components/Products.js",[],"/Users/jeff/Documents/projects/doe-dash/src/components/ProductCard.js",["177"],"import React, { useState } from \"react\";\nimport AddOnModal from \"react-modal\";\nimport AddOns from \"./AddOns\";\n\nconst ProductCard = ({ product, orderItem }) => {\n  const [modalOpen, setModalOpen] = useState(false);\n\n  const modalStyles = {\n    content: {\n      top: \"50%\",\n      left: \"50%\",\n      right: \"auto\",\n      bottom: \"auto\",\n      width: \"45%\",\n      height: \"90%\",\n      borderRadius: \"2%\",\n      marginRight: \"-50%\",\n      transform: \"translate(-50%, -50%)\",\n    },\n  };\n\n  AddOnModal.setAppElement(\"#modal\");\n\n  return (\n    <>\n      <div className=\"product-card\" onClick={() => setModalOpen(true)}>\n        <img src={product.photo} />\n        <div className=\"product-card-text\">\n          <p className=\"card-name\">{product.name}</p>\n          <p className=\"card-text\">{product.description}</p>\n          <p className=\"card-text\">{`$${product.price}`}</p>\n        </div>\n      </div>\n      <AddOnModal\n        isOpen={modalOpen}\n        onRequestClose={() => setModalOpen(false)}\n        style={modalStyles}\n      >\n        <AddOns\n          product={product}\n          setModalOpen={setModalOpen}\n          orderItem={orderItem}\n        />\n      </AddOnModal>\n    </>\n  );\n};\n\nexport default ProductCard;\n",["178","179"],"/Users/jeff/Documents/projects/doe-dash/src/components/CategoryBox.js",["180"],"import React from \"react\";\n//import chicken from \"../assets/icons/chicken.jpg\";\nconst images = require.context(\"../assets/icons/categories/\", true);\n\nconst CategoryBox = ({ category, handleCategoryClick }) => {\n  const imageUrl = images(`./${category.id}.jpg`).default;\n\n  return (\n    <div className=\"category-box\" onClick={() => handleCategoryClick(category)}>\n      <img src={imageUrl} />\n      <p>{category.title}</p>\n    </div>\n  );\n};\n\nexport default CategoryBox;\n","/Users/jeff/Documents/projects/doe-dash/src/components/CategoryBar.js",[],"/Users/jeff/Documents/projects/doe-dash/src/components/HelpPage.js",[],"/Users/jeff/Documents/projects/doe-dash/src/components/Category.js",[],"/Users/jeff/Documents/projects/doe-dash/src/data/categories.js",[],"/Users/jeff/Documents/projects/doe-dash/src/components/Store2Prods.js",["181","182","183"],"/Users/jeff/Documents/projects/doe-dash/src/components/CartSlider.js",["184"],"import React from \"react\";\nimport Cart from \"./Cart\";\n\nconst CartSlider = (props) => {\n  return (\n    <div className=\"cart-slider-bgg\" onClick={() => props.onRequestClose()}>\n      <div\n        className=\"cart-slider\"\n        style={{\n          transition: \"transform .5s\",\n          transform: props.isOpen ? \"translateX(0)\" : \"translateX(20vw)\",\n        }}\n      >\n        {props.children}\n      </div>\n    </div>\n  );\n};\n\nexport default CartSlider;\n\n/*\n        opacity: props.isOpen ? 1 : 0,\n\n*/\n","/Users/jeff/Documents/projects/doe-dash/src/components/MenuSlider.js",["185"],"import Menu from \"./Menu\";\n\nconst MenuSlider = (props) => {\n  return (\n    <div className=\"menu-slider-bgg\" onClick={() => props.onRequestClose()}>\n      <div\n        className=\"menu-slider\"\n        style={{\n          transition: \"transform .5s\",\n          transform: props.isOpen ? \"translateX(0)\" : \"translateX(-25vw)\",\n        }}\n      >\n        {props.children}\n      </div>\n    </div>\n  );\n};\n\nexport default MenuSlider;\n","/Users/jeff/Documents/projects/doe-dash/src/components/AddOns.js",["186","187","188","189","190"],"import React, { useState } from \"react\";\nimport closex from \"../assets/icons/closex.svg\";\nimport decrease from \"../assets/icons/decrease.svg\";\nimport increase from \"../assets/icons/increase.svg\";\nimport AddOn from \"./AddOn\";\n\nconst AddOns = ({ product, setModalOpen, orderItem }) => {\n  const [quantity, setQuantity] = useState(1);\n  const [addOnsOrdered, setAddOnsOrdered] = useState({});\n  const [totalCost, setTotalCost] = useState(product.price);\n\n  const calcTotal = () => {};\n\n  const formatOrderAndSubmit = () => {\n    product.addOnsOrdered = addOnsOrdered;\n    product.totalCost = totalCost; //  this is the cost for ONE item\n    product.quantity = quantity;\n    orderItem(product);\n    setModalOpen(false);\n  };\n\n  return (\n    <div className=\"add-ons\">\n      <img src={closex} onClick={() => setModalOpen(false)} />\n      <h1>{product.name}</h1>\n      <img className=\"add-ons-img\" src={product.photo} 